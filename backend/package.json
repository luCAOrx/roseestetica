{
  "name": "roseestetica-app",
  "version": "1.0.0",
  "description": "Api para gerenciamento de atendimentos",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "test": "cross-env NODE_ENV=test jest",
    "development:knex:seed": "knex --knexfile knexfile.ts seed:run",
    "development:knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "production:knex:migrate": "knex --knexfile knexfile.ts migrate:latest --env production",
    "production:knex:seed": "knex --knexfile knexfile.ts seed:run --env production"
  },
  "author": "luCAO",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.935.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.2.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.4.10",
    "nodemailer-express-handlebars": "^5.0.0",
    "pg": "^8.7.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/multer-s3": "^3.0.0",
    "@types/nodemailer": "^6.4.1",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.2",
    "faker": "^5.5.3",
    "jest": "^28.1.3",
    "nodemon": "^2.0.4",
    "sqlite3": "^5.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.2.3"
  }
}
